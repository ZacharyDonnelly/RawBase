{
  "name": "@rawbase/auth-server",
  "version": "1.2.1",
  "private": true,
  "description": "Auth microservice client for RawBase",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "yarn rimraf dist",
    "codegen": "yarn graphql-codegen",
    "debug": "yarn nodemon -e ts,js,graphql -x node --inspect -r dotenv/config src/index.js",
    "dev": "yarn concurrently 'yarn nodemon' 'tsc --watch --preserveWatchOutput' -n node,ts -c green.bgBlack,white.bgBlue",
    "lint:eslint": "eslint --fix --ignore-path .gitignore --ext .ts",
    "lint:prettier": "prettier --write --loglevel warn \"**/*.{js,ts,json}\"",
    "start": "nodemon --config nodemon.json server.ts"
  },
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@graphql-tools/graphql-file-loader": "7.5.17",
    "@graphql-tools/load": "7.8.14",
    "@graphql-tools/schema": "8.5.1",
    "apollo-server": "3.13.0",
    "apollo-server-core": "3.9.0",
    "apollo-server-express": "3.13.0",
    "bcrypt": "5.1.1",
    "chalk": "5.3.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "express": "4.21.1",
    "express-graphql": "0.12.0",
    "graphql": "16.5.0",
    "graphql-import": "1.0.2",
    "jsonwebtoken": "8.5.1",
    "sequelize": "6.21.6",
    "sqlite3": "5.0.11"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.25.9",
    "@graphql-codegen/cli": "2.16.5",
    "@graphql-codegen/typescript": "2.8.8",
    "@graphql-codegen/typescript-resolvers": "2.7.13",
    "@types/bcrypt": "5.0.0",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.12",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.14",
    "@types/jsonwebtoken": "8.5.9",
    "@types/morgan": "1.9.3",
    "@types/node": "18.19.59",
    "concurrently": "7.6.0",
    "dotenv": "16.4.5",
    "morgan": "1.10.0",
    "nodemon": "2.0.20",
    "reflect-metadata": "0.2.2",
    "ts-node": "10.9.2",
    "tslib": "2.8.0",
    "typescript": "4.9.5"
  },
  "peerDepenedencies": {
    "graphql": "^15.3.0"
  }
}
