{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "16",
          "esmodules": false
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "overrides": [
    {
      "test": "**/*.d.ts",
      "presets": [["@babel/preset-env", { "targets": { "esmodules": true } }]]
    }
  ]
}
