{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable"],
    "outDir": "dist",
    "baseUrl": "./src",
    "target": "es5",
    "allowJs": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["*"],
      "@assets": ["assets/*"],
      "@components": ["components/*"]
    }
  },
  "include": ["src"]
}
