{
  "name": "rawbase",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "dev": "yarn run webpack --open --hot",
    "dev:debug": "yarn run debug",
    "lint-staged": "cat .lintstagedrc.json | npx lint-staged --config -",
    "lint:all": "npm-run-all --parallel lint:*",
    "lint:eslint": "eslint --fix --ignore-path .gitignore --ext .ts,.tsx",
    "lint:prettier": "prettier --write --loglevel warn \"**/*.{js,ts,json,scss,tsx}\"",
    "lint:stylelint": "stylelint .stylelintrc.json --fix --ignore-path .gitignore",
    "prepare": "husky install",
    "prod": "yarn run webpack --env=production",
    "prod:analyze": "yarn run prod --env.presets analyze",
    "prod:compress": "yarn run prod --env=production compress",
    "prod:debug": "yarn run debug --env=production",
    "prod:ts": "yarn run prod --env.presets typescript",
    "start": "webpack-dev-server --env development --hot --open",
    "webpack": "webpack"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "^28.0.3",
    "babel-loader": "8.2.5",
    "babel-plugin-named-asset-import": "0.4.0-next.14",
    "babel-plugin-transform-typescript": "^7.0.0-beta.3",
    "babel-preset-react-app": "^10.1.0-next.14",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^9.2.0",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.7",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss-loader": "^6.2.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.1",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "redux-persist": "^6.0.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "workbox-webpack-plugin": "^6.5.3"
  },
  "devDependencies": {
    "@stylelint/prettier-config": "^2.0.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "classnames": "^2.3.1",
    "postcss-html": "^1.4.1",
    "sass": "^1.51.0",
    "stylelint": "^14.8.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3",
    "stylelint-scss": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2"
  }
}
