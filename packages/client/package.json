{
  "name": "@rawbase/client",
  "version": "1.0.2",
  "private": true,
  "license": "MIT",
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yml",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "dev:debug": "yarn run debug",
    "lint-staged": "cat .lintstagedrc.json | npx lint-staged --config -",
    "lint:all": "npm-run-all --parallel lint:*",
    "lint:eslint": "eslint --fix --ignore-path .gitignore --ext .ts,.tsx",
    "lint:prettier": "prettier --write --loglevel warn \"**/*.{js,ts,json,scss,tsx}\"",
    "lint:stylelint": "stylelint .stylelintrc.json --fix --ignore-path .gitignore",
    "prepare": "husky install",
    "prod": "yarn run webpack --env=production",
    "prod:analyze": "yarn run prod --env.presets analyze",
    "prod:compress": "yarn run prod --env=production compress",
    "prod:debug": "yarn run debug --env=production",
    "prod:ts": "yarn run prod --env.presets typescript",
    "start": "webpack-dev-server --env development --hot --open"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@rawbase/auth-server": "^1.2.1",
    "@rawbase/common": "^1.0.0",
    "@reduxjs/toolkit": "1.8.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.2.6",
    "awesome-typescript-loader": "5.2.1",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "28.1.3",
    "babel-loader": "8.2.5",
    "babel-plugin-named-asset-import": "0.3.8",
    "babel-plugin-transform-typescript": "7.0.0-beta.3",
    "babel-preset-react-app": "10.0.1",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "10.0.0",
    "css-loader": "6.7.1",
    "cssnano": "5.1.13",
    "file-loader": "6.2.0",
    "graphql": "16.5.0",
    "graphql-tag": "2.12.6",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.1",
    "normalize.css": "8.0.1",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "postcss-loader": "7.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.2",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0",
    "redux-persist": "6.0.0",
    "sass-loader": "13.0.2",
    "style-loader": "3.3.1",
    "ts-loader": "9.3.1",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "4.1.1",
    "webpack": "5.73.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3",
    "webpack-merge": "5.8.0",
    "webpack-node-externals": "3.0.0",
    "workbox-webpack-plugin": "6.5.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.4",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.5.1",
    "@graphql-codegen/typescript-operations": "2.4.3",
    "@graphql-codegen/typescript-react-apollo": "3.2.17",
    "@stylelint/prettier-config": "2.0.0",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "postcss-html": "1.4.1",
    "stylelint": "14.9.1",
    "stylelint-a11y": "1.2.3",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-no-unsupported-browser-features": "5.0.4",
    "stylelint-scss": "4.2.0",
    "tsconfig-paths-webpack-plugin": "3.5.2"
  },
  "peerDependencies": {
    "webpack": "^4.0.0"
  }
}
